.INFO

name: "Deeeelay"

.PARAMETERS

b0: (name: "b0", default: 1.0, min: -2.0, max: 2.0)

.RESOURCES

delay1: (type: "delay", delay: 1)
delay2: (type: "delay", delay: 1)

.CODE

delay_read c1 $delay1
delay_read c2 $delay2
madd c0 c1 [1.0]  c0
madd c0 c2 [0.25] c0
delay_write $delay1 c0
delay_write $delay2 c1
